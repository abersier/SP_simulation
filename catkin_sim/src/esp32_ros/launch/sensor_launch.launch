<launch>
    <!-- Run the roscore -->
    <master auto="start"/>

    <!-- Run the C++ rosnode -->
    <node name="sensor_listener" pkg="esp32_ros" type="sensor_listener" output="screen"/>

    <!-- Run the pyserial node -->
    <node name="serial_node" pkg="rosserial_python" type="serial_node.py" output="screen">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="57600"/>
    </node>
</launch>